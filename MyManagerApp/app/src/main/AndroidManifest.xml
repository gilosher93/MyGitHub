<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gil.mymanagerapp" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ManagerActivity"
            android:label="@string/title_activity_manager" >
        </activity>
        <activity
            android:name=".RstManageActivity"
            android:label="@string/title_activity_rst_manage" >
        </activity>
        <activity
            android:name=".WorkersManageActivity"
            android:label="@string/title_activity_workers_list" >
        </activity>
    </application>

</manifest>
